<Window x:Class="ISIP_FrameworkGUI.Windows.mathematical_function"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="mathematical_function" Height="600" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="430*" />
            <RowDefinition Height="131*" />
        </Grid.RowDefinitions>
        <Canvas Name="mainCanvas"/>
        <TextBlock Grid.Row="1" Height="37" HorizontalAlignment="Left" Margin="46,26,0,0" Name="textBlock1" Text="Enter a value for K :" VerticalAlignment="Top" Width="118" />
        <TextBlock Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="46,59,0,0" Name="textBlock2" Text="Enter a value for S :" VerticalAlignment="Top" Width="104" DataContext="{Binding}" />
        <TextBox Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="157,23,0,0" Name="input_K" VerticalAlignment="Top" Width="120" />
        <TextBox Grid.Row="1" Height="23" HorizontalAlignment="Left" Margin="157,56,0,0" Name="input_S" VerticalAlignment="Top" Width="120" />
        <Button Content="Draw !" Grid.Row="1" Height="53" HorizontalAlignment="Left" Margin="330,26,0,0" Name="do_it" VerticalAlignment="Top" Width="119" Click="do_it_Click" />
    </Grid>
</Window>
